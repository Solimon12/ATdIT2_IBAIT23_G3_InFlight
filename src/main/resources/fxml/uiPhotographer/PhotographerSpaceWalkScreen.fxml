<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="612.0" prefWidth="929.0" spacing="20.0" styleClass="bodybg" stylesheets="@../../css/PhotographerBackgroundStyle.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="58.0" prefWidth="890.0">
      <children>
         <Button fx:id="photographerCheckOutBackButton" mnemonicParsing="false" onAction="#backToPhotographerStartScreen" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: lightgray;" text="Back">
            <effect>
               <DropShadow />
            </effect>
         </Button>
      </children>
   </HBox>
   <Label prefHeight="50.0" />
   <Label prefWidth="382.0" text="Step Out to Frame the Elite" textFill="WHITE">
      <font>
         <Font name="System Bold" size="30.0" />
      </font>
   </Label>
   <Button fx:id="photographerCheckOutButton" mnemonicParsing="false" onAction="#checkInOrOutPhotographer" prefHeight="100.0" prefWidth="150.0" style="-fx-background-color: white;" text="Check Out">
      <effect>
         <DropShadow />
      </effect>
      <font>
         <Font size="20.0" />
      </font>
   </Button>
   <Label prefHeight="100.0" />
</VBox>
