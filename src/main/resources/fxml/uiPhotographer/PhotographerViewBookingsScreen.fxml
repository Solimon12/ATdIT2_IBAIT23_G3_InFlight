<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="612.0" prefWidth="929.0" styleClass="bodybg" stylesheets="@../../css/PhotographerBackgroundStyle.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox fx:id="photographerReviewBookingsHBox" prefHeight="58.0" prefWidth="890.0">
      <children>
         <Button fx:id="viewBookingsBackButton" mnemonicParsing="false" onAction="#backToPhotographerStartScreen" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: lightgray;" text="Back">
            <effect>
               <DropShadow />
            </effect>
         </Button>
      </children>
   </HBox>
   <HBox alignment="CENTER" prefHeight="87.0" prefWidth="890.0" spacing="20.0">
      <children>
         <Label alignment="CENTER" prefHeight="50.0" prefWidth="200.0" text="Package" textFill="WHITE">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
         </Label>
         <Label alignment="CENTER" prefHeight="50.0" prefWidth="200.0" text="Time" textFill="WHITE">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
         </Label>
         <Label alignment="CENTER" prefHeight="50.0" prefWidth="200.0" text="Passenger" textFill="WHITE">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
         </Label>
         <Label alignment="CENTER" prefHeight="50.0" prefWidth="200.0" text="Approval" textFill="WHITE">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
         </Label>
      </children>
   </HBox>
   <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" style="-fx-border-color: white;">
      <VBox fx:id="photographerBookingReviewContainer" spacing="15.0" alignment="CENTER" />
   </HBox>
</VBox>
