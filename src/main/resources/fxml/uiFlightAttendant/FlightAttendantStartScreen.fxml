<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="612.0" prefWidth="929.0" styleClass="bodybg" stylesheets="@../../css/FlightAttendantBackgroundStyle.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label prefHeight="50.0" />
   <HBox alignment="CENTER" prefHeight="200.0" prefWidth="890.0" spacing="50.0">
      <children>
          <Button fx:id="viewPassengersButton" onAction="#switchToFlightAttendantViewPassengersScreen" prefHeight="150.0" prefWidth="200.0" style="-fx-background-color: white;" text="View Passengers" textAlignment="CENTER">
            <font>
               <Font size="20.0" />
            </font>
            <effect>
               <DropShadow />
            </effect></Button>
         <Button fx:id="viewPhotographerButton" onAction="#switchToFlightAttendantViewPhotographerScreen" prefHeight="150.0" prefWidth="200.0" style="-fx-background-color: white;" text="View Photographer" textAlignment="CENTER">
            <font>
               <Font size="20.0" />
            </font>
            <effect>
               <DropShadow />
            </effect></Button>
      </children>
      <effect>
         <DropShadow />
      </effect>
   </HBox>
   <HBox alignment="CENTER" prefHeight="200.0" prefWidth="890.0" spacing="50.0">
      <children>
         <Button fx:id="inventoryButton" alignment="CENTER" onAction="#switchToFlightAttendantInventoryMenuScreen" prefHeight="150.0" prefWidth="200.0" style="-fx-background-color: white;" text="Inventory" textAlignment="CENTER">
            <font>
               <Font size="20.0" />
            </font>
            <effect>
               <DropShadow />
            </effect>
         </Button>
         <Button fx:id="pilotMessengerButton" alignment="CENTER" mnemonicParsing="false" onAction="#switchToFlightAttendantMessagePilotScreen" prefHeight="150.0" prefWidth="200.0" style="-fx-background-color: white;" text="Message&#10;Pilot&#10;" textAlignment="CENTER">
            <font>
               <Font size="20.0" />
            </font>
            <effect>
               <DropShadow />
            </effect></Button>
      </children>
      <effect>
         <DropShadow />
      </effect>
   </HBox>
</VBox>
